      
<link href="../assets/css/pin.css?v=1990" rel="stylesheet" type="text/css">
<div id="pin-code-container" class="row" style="display: none">
    <div class="col-md-8 col-md-offset-2 col-lg-8 col-lg-offset-2 col-sm-8 col-sm-offset-2">
        <div class="login-panel panel panel-etnx">
            <div class="panel-heading" style="text-align: center">
                {% for coin in coins %}
                    <img height="40px" width="40px" src="../img/{{coin}}.png" />
                {% endfor %}
            </div>
            <div class="panel-body col-md-12 col-lg-12 col-sm-12">
                <div class="alert alert-danger" style="display: none">
                </div>
                <div role="form col-sm-12 col-md-12 col-lg-12">
                    <fieldset>
                        <div id="pin-form" class="form-group col-md-12 col-lg-12 col-sm-12" align="center">
                            <input id="digit-1" class="form-control inputDigit" type="password" style="width: 34px; display: inline-block" disabled>
                            <input id="digit-2" class="form-control inputDigit" type="password" style="width: 34px; display: inline-block" disabled>
                            <input id="digit-3" class="form-control inputDigit" type="password" style="width: 34px; display: inline-block" disabled>
                            <input id="digit-4" class="form-control inputDigit" type="password" style="width: 34px; display: inline-block" disabled>
                            <input id="digit-5" class="form-control inputDigit" type="password" style="width: 34px; display: inline-block" disabled>&#160;&#160;<a href="#" id="del" class="ah previous round">&#8249;</a>
                        </div>                        
                        <div class="form-group col-md-12 col-lg-12 col-sm-12" align="center">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="1" type="button" class="digit btn rounded">1</button></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="2" type="button" class="digit btn rounded">2</button></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="3" type="button" class="digit btn rounded">3</button></div>
                        </div>
                        <div class="form-group col-md-12 col-lg-12 col-sm-12" align="center">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="4" type="button" class="digit btn btn-primary rounded">4</button></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="5" type="button" class="digit btn btn-primary rounded">5</button></div>
                            <div class="col-lg-4 col-md-4 col-sm-4" >
                            <button id="6" type="button" class="digit btn btn-primary rounded">6</button></div>
                        </div>
                        <div class="form-group col-md-12 col-lg-12 col-sm-12" align="center">
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="7" type="button" class="digit btn">7</button></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="8" type="button" class="digit btn">8</button></div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                            <button id="9" type="button" class="digit btn">9</button></div>
                        </div>
                        <div class="form-group col-md-12 col-lg-12 col-sm-12" align="center">
                            <div class="col-lg-4 col-lg-offset-4 col-md-4 col-md-offset-4 col-sm-4 col-sm-offset-4"><br>
                            <button id="0" type="button" class="digit btn btn-lg btn-primary rounded">0</button>
                            </div>
                        </div>
                        <div class="col-lg-4 col-lg-offset-8 col-md-4 col-md-offset-8 col-sm-4 col-sm-offset-8" align="center">
                        <button id="pin-code" type="button" class="digit btn btn-lg btn-primary rounded">Login</button>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</div>
